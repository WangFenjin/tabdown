{
  "name": "TabDown",
  "manifest_version": 2,
  "version":"1.0.0",
  "description": "Open new tab with Markdown Editor",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png",
    "256": "icons/256x256.png",
    "512": "icons/512x512.png",
    "1024": "icons/1024x1024.png"
  },
  "background":{
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_icon": "icons/256x256.png"
  },
  "permissions": [ "tabs", "activeTab", "storage" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; child-src 'self' 'unsafe-inline'"
}
